{
  "$schema": "bilibili-danmu-blocklist-schema.json",
  "rules": [
    {
      "type": 1,
      "filter": "(草|操|🌿|艹)",
      "opened": true,
      "examples": [
        "我操",
        "太生草了",
        "🌿",
        "艹"
      ]
    },
    {
      "type": 1,
      "filter": "(顿|炖|遁|盾|挪)(出|煮)红(尘|城)",
      "opened": true,
      "examples": [
        "顿出红尘",
        "炖煮红尘",
        "遁出红尘",
        "盾出红尘",
        "挪出红尘",
        "炖出红城"
      ]
    },
    {
      "type": 1,
      "filter": "(没|难)?(蚌|崩|绷)(埠|不)?(住)?(了)?",
      "opened": true,
      "examples": [
        "蚌",
        "蚌埠住了",
        "崩不住了",
        "没绷住",
        "难蚌"
      ]
    },
    {
      "type": 1,
      "filter": "(爷|奶)(青|童)(结|回)",
      "opened": true,
      "examples": [
        "爷青结",
        "奶青回",
        "爷童回"
      ]
    },
    {
      "type": 1,
      "filter": "(一|亿)拍(即|既)合",
      "opened": true,
      "examples": [
        "一拍即合",
        "亿拍即合",
        "亿拍既合"
      ]
    },
    {
      "type": 1,
      "filter": "((八|巴)人|迈)(八|巴|拔)(鹤|鶴|赫|河|盒|核)",
      "opened": true,
      "examples": [
        "八人八鹤",
        "八人巴赫",
        "八人拔河",
        "八人八鶴",
        "八人八盒",
        "八人八核",
        "巴人八鹤",
        "迈巴鹤经销商"
      ]
    },
    {
      "type": 1,
      "filter": "((唯|惟|但)手熟|自动挡)尔",
      "opened": true,
      "examples": [
        "唯手熟尔",
        "惟手熟尔",
        "无它，但手熟尔",
        "无他自动挡尔"
      ]
    },
    {
      "type": 1,
      "filter": "(?<!乾)坤",
      "opened": true,
      "examples": [
        "一坤年",
        "一百坤"
      ],
      "excludeExamples": [
        "林间松韵，石上泉声，静里听来，识天地自然鸣佩；草际烟光，水心云影，闲中观去，见乾坤最上文章"
      ]
    },
    {
      "type": 1,
      "filter": ".+播放.+赞",
      "opened": true,
      "examples": [
        "一播放13赞"
      ]
    },
    {
      "type": 1,
      "filter": ".+の.+",
      "opened": true,
      "examples": [
        "萨隆の关心",
        "智慧の眼神"
      ]
    },
    {
      "type": 1,
      "filter": "[Ss][Hh][Ii][Tt]",
      "opened": true,
      "examples": [
        "SHIT",
        "shit"
      ]
    },
    {
      "type": 1,
      "filter": "^啊对{3,}$",
      "opened": true,
      "examples": [
        "啊对对对"
      ]
    },
    {
      "type": 1,
      "filter": "^出院",
      "opened": true
    },
    {
      "type": 1,
      "filter": "^好耶",
      "opened": true
    },
    {
      "type": 1,
      "filter": "^桀{3,}$",
      "opened": true,
      "examples": [
        "桀桀桀"
      ]
    },
    {
      "type": 1,
      "filter": "^开播大吉",
      "opened": true
    },
    {
      "type": 1,
      "filter": "^泪(目|了)$",
      "opened": true,
      "examples": [
        "泪目",
        "泪了"
      ]
    },
    {
      "type": 1,
      "filter": "^完结撒花",
      "opened": true
    },
    {
      "type": 1,
      "filter": "^唯.真(神|史)",
      "opened": true,
      "examples": [
        "唯一真神",
        "唯一真史",
        "唯二真神"
      ]
    },
    {
      "type": 1,
      "filter": "^(爱了)+",
      "opened": true,
      "examples": [
        "爱了",
        "爱了爱了"
      ]
    },
    {
      "type": 1,
      "filter": "^(多|夺)少[?？]?",
      "opened": true,
      "examples": [
        "多少",
        "夺少?"
      ]
    },
    {
      "type": 1,
      "filter": "^(盖|迪).*(亚|迦).*$",
      "opened": true,
      "examples": [
        "盖亚",
        "盖~亚~",
        "盖亚！！！！",
        "迪迦！！！！"
      ]
    },
    {
      "type": 1,
      "filter": "^(妙|秒)啊$",
      "opened": true,
      "examples": [
        "妙啊",
        "秒啊"
      ]
    },
    {
      "type": 1,
      "filter": "^(天才|甜菜)",
      "opened": true,
      "examples": [
        "天才",
        "甜菜"
      ]
    },
    {
      "type": 1,
      "filter": "^(我|哇|蛙)(去|趣)",
      "opened": true,
      "examples": [
        "我去",
        "哇趣",
        "蛙趣"
      ]
    },
    {
      "type": 1,
      "filter": "^.厨狂喜",
      "opened": true,
      "examples": [
        "双重狂喜",
        "三厨狂喜"
      ]
    },
    {
      "type": 1,
      "filter": "^.*(丸|完)辣$",
      "opened": true,
      "examples": [
        "丸辣",
        "完辣"
      ]
    },
    {
      "type": 1,
      "filter": "^.+(万岁|萬歲)",
      "opened": true,
      "examples": [
        "人民万岁",
        "人民萬歲！！！"
      ]
    },
    {
      "type": 1,
      "filter": "^[Aa][Ww][Ss][Ll]$",
      "opened": true,
      "examples": [
        "AWSL",
        "awsl"
      ]
    },
    {
      "type": 1,
      "filter": "^[Oo][Pp]$",
      "opened": true,
      "examples": [
        "op"
      ]
    },
    {
      "type": 1,
      "filter": "^[\\d\\.\\*\\+\\-\\/\\|。\\?？\\!！,，~～;；、…哈阿啊嗨哇哦嗯嘿耶呜哒啦拉\\\\]+$",
      "opened": true,
      "examples": [
        "哈哈哈",
        "啊啊啊",
        "嗨嗨嗨",
        "呜呜呜",
        "???",
        "。。。",
        "～～～",
        "|||",
        ";;;",
        "啊?",
        "嗯?",
        "哇哦",
        "耶！",
        "*****-+",
        ".0.0.0..0",
        "………………6",
        "哒，哒哒哒哒～"
      ]
    },
    {
      "type": 1,
      "filter": "^[^\\d]+[:：][^\\d]+$",
      "opened": true,
      "examples": [
        "月亮：神金",
        "天美：我不知道啊",
        "灰太狼：6"
      ],
      "excludeExamples": [
        "3:54"
      ]
    },
    {
      "type": 1,
      "filter": "^\\d+$",
      "opened": true,
      "examples": [
        "111",
        "2233",
        "66666",
        "99999"
      ]
    },
    {
      "type": 1,
      "filter": "^h{3,}$",
      "opened": true,
      "examples": [
        "hhh",
        "hhhhhh"
      ]
    },
    {
      "type": 1,
      "filter": "^《.+》$",
      "opened": true,
      "examples": [
        "《相貌平平》",
        "《氨醛》"
      ]
    },
    {
      "type": 1,
      "filter": "奥(力|利|里)(给|怪)",
      "opened": true,
      "examples": [
        "奥里给",
        "奥力给",
        "奥利给",
        "奥利怪"
      ]
    },
    {
      "type": 0,
      "filter": "fw",
      "opened": true
    },
    {
      "type": 0,
      "filter": "感谢大自然的馈赠",
      "opened": true
    },
    {
      "type": 0,
      "filter": "鸽鸽",
      "opened": true
    },
    {
      "type": 0,
      "filter": "沟槽",
      "opened": true
    },
    {
      "type": 0,
      "filter": "火钳刘明",
      "opened": true
    },
    {
      "type": 0,
      "filter": "今夕是何年",
      "opened": true
    },
    {
      "type": 0,
      "filter": "恐怖如斯",
      "opened": true
    },
    {
      "type": 1,
      "filter": "梦开(始|启)的(地方|时候)",
      "opened": true,
      "examples": [
        "梦开始的地方",
        "梦开始的时候",
        "梦开启的地方"
      ]
    },
    {
      "type": 0,
      "filter": "命运的齿轮开始转动",
      "opened": true
    },
    {
      "type": 1,
      "filter": "念头不?通达",
      "opened": true,
      "examples": [
        "念头通达",
        "念头不通达了"
      ]
    },
    {
      "type": 0,
      "filter": "全体起立",
      "opened": true
    },
    {
      "type": 0,
      "filter": "神经，害我笑了一下",
      "opened": true,
      "examples": [
        "神经，害我笑了一下"
      ]
    },
    {
      "type": 0,
      "filter": "塔塔开",
      "opened": true
    },
    {
      "type": 1,
      "filter": "我(了|嘞)个豆",
      "opened": true,
      "examples": [
        "我了个豆",
        "我嘞个豆",
        "好超前，我了个豆"
      ]
    },
    {
      "type": 1,
      "filter": "下次也?(一|丕)定",
      "opened": true,
      "examples": [
        "下次一定",
        "下次丕定",
        "下次也不一定"
      ]
    },
    {
      "type": 0,
      "filter": "仙之人兮列如麻",
      "opened": true
    },
    {
      "type": 1,
      "filter": "永不为奴$",
      "opened": true,
      "examples": [
        "LCD 永不为奴",
        "580 永不为奴"
      ]
    },
    {
      "type": 0,
      "filter": "真神登场",
      "opened": true
    },
    {
      "type": 0,
      "filter": "只因",
      "opened": true
    },
    {
      "type": 1,
      "filter": "助力每.*一个.*梦想",
      "opened": true,
      "examples": [
        "助力每一个梦想",
        "助力每一个不知死活的梦想",
        "助力每日一个梦想"
      ]
    }
  ]
}